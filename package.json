{
  "name": "vrh-ng",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/vrh-ng/server/main.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev": "ng run vrh-ng:serve-ssr",
    "serve:ssr": "node dist/vrh-ng/server/main.js",
    "build:ssr": "ng build && ng run vrh-ng:server",
    "prerender": "ng run vrh-ng:prerender",
    "prebuild:ssr": "ngcc",
    "apigen:get-api": "curl http://localhost:4200/api-json -o api.json",
    "apigen:generate": "swagger-angular-generator -s ./api.json -d ./src/app/api",
    "apigen:clear": "rm ./api.json",
    "apigen": "npm run apigen:get-api && npm run apigen:generate && npm run apigen:clear"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/localize": "^12.2.1",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/platform-server": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/mapped-types": "^1.0.0",
    "@nestjs/ng-universal": "^5.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/platform-socket.io": "^8.0.6",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.0.6",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0-beta.2",
    "@ngrx/data": "^12.4.0",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/entity": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "@nguniversal/express-engine": "^12.1.0",
    "bootstrap": "^5.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "express": "^4.15.2",
    "mongodb": "^3.6.11",
    "nestjs-minio": "^2.1.10",
    "ngx-plyr": "^4.0.0",
    "ngx-socket-io": "^4.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-oauth2": "^1.6.0",
    "plyr": "^3.6.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "saslprep": "^1.0.3",
    "swagger-ui-express": "^4.1.6",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@nguniversal/builders": "^12.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.8.0",
    "@types/minio": "^7.0.9",
    "@types/mongodb": "^3.6.11",
    "@types/node": "^12.11.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-oauth2": "^1.4.11",
    "@types/passport-vkontakte": "^1.3.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "swagger-angular-generator": "^4.2.2",
    "typeorm": "^0.2.37",
    "typescript": "~4.3.5"
  }
}
